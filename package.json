{
  "name": "sd-parsers-api",
  "version": "1.0.0",
  "description": "A REST API to read metadata from images created by Stable Diffusion",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "tsc --outDir ./dist/src/",
    "dev": "tsc --watch",
    "start": "node dist/src/server.js",
    "start:dev": "npm run build && npm start",
    "vercel-build": "npm run build",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "stable-diffusion",
    "metadata",
    "image-parsing",
    "ai",
    "api",
    "server",
    "rest",
    "automatic1111",
    "comfyui",
    "fooocus",
    "invokeai",
    "novelai"
  ],
  "author": "Deepratna Awale",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "exifr": "^7.1.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "png-chunks-extract": "^1.0.0",
    "sharp": "^0.33.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.0.0",
    "@types/png-chunks-extract": "^1.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deepratna-awale/sd-parsers.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
